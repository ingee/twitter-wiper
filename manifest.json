{
  "description": "Wipe down anoying toast message from twitter timeline",
  "manifest_version": 2,
  "name": "TwitterWiper",
  "version": "1.0",
  "homepage_url": "https://github.com/ingee/twitter-wiper.git",
  "icons": {
    "64": "icons/icons8-wiper-64.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*"],
      "js": ["wipedown.js"]
    }
  ]

}
